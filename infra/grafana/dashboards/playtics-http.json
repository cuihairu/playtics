{
  "uid": "playtics-http",
  "title": "Playtics HTTP Server",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Req Rate (req/s)",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
      "targets": [
        {"expr": "sum(rate(http_server_requests_seconds_count[1m])) by (status)", "legendFormat": "{{status}} (micrometer)"},
        {"expr": "sum(rate(http_server_duration_seconds_count[1m])) by (status)", "legendFormat": "{{status}} (otel)"}
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP Duration P95 (s)",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[5m])) by (le))", "legendFormat": "p95 (micrometer)"},
        {"expr": "histogram_quantile(0.95, sum(rate(http_server_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p95 (otel)"}
      ]
    }
  ]
}
