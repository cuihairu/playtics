{
  "uid": "playtics-overview",
  "title": "Playtics Overview",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "OTLP Accepted Spans",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {"expr": "sum(rate(otelcol_receiver_accepted_spans[5m])) by (receiver)", "legendFormat": "{{receiver}}"}
      ]
    },
    {
      "type": "timeseries",
      "title": "OTLP Accepted Metrics",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {"expr": "sum(rate(otelcol_receiver_accepted_metric_points[5m])) by (receiver)", "legendFormat": "{{receiver}}"}
      ]
    },
    {
      "type": "timeseries",
      "title": "OTLP Accepted Logs",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {"expr": "sum(rate(otelcol_receiver_accepted_logs[5m])) by (receiver)", "legendFormat": "{{receiver}}"}
      ]
    },
    {
      "type": "stat",
      "title": "Collector Uptime (s)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {"expr": "max(otelcol_process_uptime)"}
      ]
    }
  ]
}
